<interface>
  <!-- interface-name example -->
  <requires lib="gtk" version="4.6"/>
  <requires lib="gio" version="2.0"/>
  <template class="LemmyDesktopMainWindow" parent="GtkApplicationWindow">
    <property name="default-height">500</property>
    <property name="default-width">700</property>
    <property name="name">FlyBy</property>
    <property name="show-menubar">True</property>
    <property name="title">Lemmy</property>
    <child>
      <object class="GtkPaned" id="paned1">
        <property name="end-child">
          <object class="GtkPaned" id="paned2">
            <property name="end-child">
              <object class="GtkLabel">
                <property name="label">post</property>
              </object>
            </property>
            <property name="start-child">
              <object class="GtkBox">
                <property name="hexpand">True</property>
                <property name="orientation">vertical</property>
                <property name="vexpand">True</property>
                <child>
                  <object class="GtkScrolledWindow">
                    <property name="hexpand">True</property>
                    <property name="vexpand">True</property>
                    <child>
                      <object class="GtkColumnView" id="posts_list"/>
                    </child>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </property>
        <property name="resize-start-child">False</property>
        <property name="shrink-start-child">False</property>
        <property name="start-child">
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <property name="width-request">200</property>
            <child>
              <object class="GtkLabel">
                <property name="hexpand">True</property>
                <property name="label">comms</property>
                <property name="vexpand">True</property>
              </object>
            </child>
          </object>
        </property>
      </object>
    </child>
  </template>
  <menu id="app_menu">
    <item>
      <attribute name="label">&amp;Settings</attribute>
    </item>
    <item>
      <attribute name="label">&amp;Quit</attribute>
    </item>
  </menu>
</interface>
