<schemalist>
  <schema id="com.github.albert-tomanek.lemmy-desktop" path="/com/github/albert-tomanek/lemmy-desktop/">
    <key name="blocked-instances" type="as">
      <default>[]</default>
    </key>
    <key name="blocked-words" type="as">
      <default>[]</default>
    </key>
    <key name="blocked-comms" type="as">
      <default>[]</default>
      <description>Format: "!&lt;comm&gt;@&lt;instance&gt;"</description>
    </key>

    <key name="jwt" type="s">
      <default>""</default>
      <description>Token to use with API.</description>
    </key>

    <key name="paned1-pos" type="i">
      <default>200</default>
      <!-- <summary>How much the red channel should be boosted by in anaglyphs.</summary>
      <description>
        In some anaglyphic glasses the red filter is darker than the blue filter. Boosting the red channel can compensate for this. This property is the coefficient that the red channel is multiplied by.
      </description> -->
    </key>

    <key name="paned2-pos" type="i">
      <default>300</default>
    </key>

  </schema>
</schemalist>